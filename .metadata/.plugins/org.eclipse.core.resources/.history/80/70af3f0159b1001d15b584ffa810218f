package emonics.hr.model;

import javax.xml.bind.annotation.XmlRootElement;

import lombok.Data;

@Data @XmlRootElement
public class Employee {
	private int id; 
	private String name;
	private double salary;
	private int deptId;
	private int mgrId;
	
	public Employee(int id, String name, double salary, int deptId, int mgrId) {
		this.id = id;
		this.name = name;
		this.salary = salary;
		this.deptId = deptId;
		this.mgrId = mgrId;
	}

	public Employee() {
		
	}
	
}
