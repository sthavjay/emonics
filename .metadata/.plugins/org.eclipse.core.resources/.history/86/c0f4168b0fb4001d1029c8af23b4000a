package com.all;

@Stateless
public class Alpha implements UseMe {
	int id;
	String name;
	Beta fulladdress;
	
	 Alpha alphaObject = new Alpha();
	 UseMe betaObject = new UseMe();
	 
	 public static void main(String[] args) {
	 
 alphaObject.id= 1;
 alphaObject.name = "Bijaya";
 betaObject.fulladress.zc = 92801;
 alphaObject.fulladdress.address = "Magnolia";
 
 
	 }
}
